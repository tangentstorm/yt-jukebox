{
  "name": "yt-jukebox",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "youtube jukebox",
  "homepage_url": "http://tangentcode.com/ytk",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["src/events.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "youtube karaoke jukebox"
  },
  "permissions": [
    "tabs", "activeTab", "https://www.youtube.com/*"
  ],
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["src/inject.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": ["src/player.js"]
}
