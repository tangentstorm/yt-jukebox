{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "v": { ".read": true },
    "u": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        ".validate": "newData.hasChildren(['v','a','t'])",
        "v": { ".validate": "newData.val() !== ''" },
        "a": { },
        "t": { }
      }
    }
  }
}
